extends ../base
include ../mixin/video

block content
	div.profile__data
		if isHeroku
			img.header__avatar(src=loggedInUser.avatarUrl, crossorigin).avatar
		else
			img.header__avatar(src=`/${loggedInUser.avatarUrl}`, crossorigin).avatar
		h3=user.name
	div.video-grid
		each video in user.videos
			+video(video)
		else
			span.empty__message No videos found 